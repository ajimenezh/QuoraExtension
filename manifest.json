{
  "manifest_version": 2,

  "name": "Quora Extension",
  "description": "",
  "version": "1.0",

  "permissions": [
    "http://www.quora.com/",
    "http://qsc.is.quoracdn.net/",
    "contextMenus",
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": ["script.js", "database.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}